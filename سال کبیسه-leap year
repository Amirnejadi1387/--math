#include <stdio.h>
int main (){
int year;
printf("enter the desired year\n");
sfanf("%d",&year);
if(year%400==0){
printf("it is a leap year \n");
}else if(year%100==0){
printf("The year is not a leap year\n");
}else if (year%4==0){
printf("it is a leap year\n");
}else{
printf("the year is not a leap year");
}
}
